<mvc:View 
    xmlns="sap.m"
    xmlns:mvc="sap.ui.core.mvc"
    displayBlock="true" 
    controllerName="webapp.controller.app" 
    >

     <App>
        <pages>
            <Page title="Agenda">
                <content>
                    <IconTabBar>
                        <items>
                        <IconTabFilter text="Contatos">
                                <Toolbar>
                        <SearchField />
                    </Toolbar>
                    	<Panel width="auto" class="sapUiResponsiveMargin">
                                <headerToolbar>
                                    <Toolbar height="3rem">
                                        <Title text="Todos os Contatos:"/>
                                    <ToolbarSpacer />   
                                    </Toolbar>
                                </headerToolbar>
                        </Panel>
 
                        </IconTabFilter>
                        <IconTabFilter text="Adicionar">
                        </IconTabFilter>
                        <IconTabFilter text="Alterar">
                        </IconTabFilter>
                        <IconTabFilter text="Remover">
                        </IconTabFilter>
                               
                        </items>
                    </IconTabBar>
                </content>               
            </Page>
        </pages>
    </App>
</mvc:View>
